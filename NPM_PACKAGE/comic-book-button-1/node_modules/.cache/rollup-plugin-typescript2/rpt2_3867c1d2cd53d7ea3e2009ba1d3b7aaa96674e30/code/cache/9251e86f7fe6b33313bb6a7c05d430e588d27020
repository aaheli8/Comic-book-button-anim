{"code":"import { __assign } from \"tslib\";\r\nimport { jsx as _jsx, jsxs as _jsxs } from \"react/jsx-runtime\";\r\nimport { useState } from \"react\";\r\nimport { motion } from \"framer-motion\";\r\nimport { buttonAnim, clickAnim } from \"./ComicButton.anim\";\r\nimport style from \"./ComicButton.module.css\";\r\nvar randomNo = function (min, max) {\r\n    return min + Math.floor(Math.random() * (max - min));\r\n};\r\nexport default function ComicButton(_a) {\r\n    var buttonName = _a.buttonName, hoverBtnName = _a.hoverBtnName, focusBtnName = _a.focusBtnName;\r\n    var _b = useState(buttonName), btnName = _b[0], setBtnName = _b[1];\r\n    return (_jsxs(motion.button, __assign({ type: \"button\", className: style.btn, initial: \"init\", animate: \"init\", whileHover: \"hover\", whileTap: \"tap\", whileFocus: \"focus\", variants: buttonAnim, custom: randomNo(6, 12), onMouseEnter: function () { return setBtnName(hoverBtnName); }, onMouseLeave: function () { return setBtnName(buttonName); } }, { children: [btnName, _jsx(motion.div, { className: style.click, variants: clickAnim, custom: randomNo(-50, 50) })] })));\r\n}\r\nComicButton.defaultProps = {\r\n    buttonName: \"BUTTON\",\r\n    hoverBtnName: \"BUTTON\",\r\n};\r\n","references":["D:/ProgramFolder/Open_source/comic-book-button/node_modules/@types/react/index.d.ts","D:/ProgramFolder/Open_source/comic-book-button/node_modules/framer-motion/dist/index.d.ts","D:/ProgramFolder/Open_source/comic-book-button/stories/ComicButton.anim.tsx"]}
